{
  "name": "agentgraph-ai",
  "version": "0.2.0",
  "description": "The Memory Layer for AI Agents - Track, visualize, and share context between AI agents",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "agents",
    "llm",
    "tracking",
    "observability",
    "memory",
    "langchain",
    "openai",
    "anthropic"
  ],
  "author": "ICE-CUBA",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ICE-CUBA/AgentGraph.git",
    "directory": "js"
  },
  "homepage": "https://github.com/ICE-CUBA/AgentGraph#readme",
  "bugs": {
    "url": "https://github.com/ICE-CUBA/AgentGraph/issues"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
